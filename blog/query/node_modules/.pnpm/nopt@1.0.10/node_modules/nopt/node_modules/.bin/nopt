#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/epikem/sync/dev/shared/tips/study/msa-lec/blog/query/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/node_modules:/Users/epikem/sync/dev/shared/tips/study/msa-lec/blog/query/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/node_modules:/Users/epikem/sync/dev/shared/tips/study/msa-lec/blog/query/node_modules/.pnpm/nopt@1.0.10/node_modules:/Users/epikem/sync/dev/shared/tips/study/msa-lec/blog/query/node_modules/.pnpm/node_modules:/Users/epikem/sync/dev/shared/tips/study/msa-lec/blog/query/node_modules:/Users/epikem/sync/dev/shared/tips/study/msa-lec/blog/node_modules:/Users/epikem/sync/dev/shared/tips/study/msa-lec/node_modules:/Users/epikem/sync/dev/shared/tips/study/node_modules:/Users/epikem/sync/dev/shared/tips/node_modules:/Users/epikem/sync/dev/shared/node_modules:/Users/epikem/sync/dev/node_modules:/Users/epikem/sync/node_modules:/Users/epikem/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/epikem/sync/dev/shared/tips/study/msa-lec/blog/query/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/node_modules:/Users/epikem/sync/dev/shared/tips/study/msa-lec/blog/query/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/node_modules:/Users/epikem/sync/dev/shared/tips/study/msa-lec/blog/query/node_modules/.pnpm/nopt@1.0.10/node_modules:/Users/epikem/sync/dev/shared/tips/study/msa-lec/blog/query/node_modules/.pnpm/node_modules:/Users/epikem/sync/dev/shared/tips/study/msa-lec/blog/query/node_modules:/Users/epikem/sync/dev/shared/tips/study/msa-lec/blog/node_modules:/Users/epikem/sync/dev/shared/tips/study/msa-lec/node_modules:/Users/epikem/sync/dev/shared/tips/study/node_modules:/Users/epikem/sync/dev/shared/tips/node_modules:/Users/epikem/sync/dev/shared/node_modules:/Users/epikem/sync/dev/node_modules:/Users/epikem/sync/node_modules:/Users/epikem/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nopt.js" "$@"
else
  exec node  "$basedir/../../bin/nopt.js" "$@"
fi
